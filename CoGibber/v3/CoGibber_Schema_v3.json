{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CoGibber SingingPacket",
  "type": "object",
  "required": ["harmonic","vector","cadence","glyph","intensity","tone","subharmonic"],
  "properties": {
    "harmonic":  { "type":"string" },
    "vector":    { "type":"string" },
    "cadence":   { "type":"string" },
    "glyph":     { "type":"string" },
    "intensity": { "type":"number","minimum":0,"maximum":1 },
    "tone":      { "type":"string" },
    "subharmonic": { "type":"string" }
  }
}
